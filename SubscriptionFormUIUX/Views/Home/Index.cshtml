				CCHolderName: $("#toSubmitCCCardholderName").val(),
				AgreeToTerms: $("#toSubmitAgreeToTerms").val() == "yes" ? true : false

			}
			$("#bgFade").show();
			var requestURL = "";
			$.ajax({
				type: "POST",
				contentType: "application/json; charset=utf-8",
				url: requestURL,
				data: JSON.stringify(regData),
				dataType: "text",
				success: function (data) {
					$("#bgFade").hide();
					var result = $.parseJSON(data);
					if (result['status'] == 'success') {
						//new redirect page
						var nDate = new Date().toISOString();					//Todays date: ISO-8601 : YYYY-MM-DDTHH:mm:ss.sssZ
						var todaysDateForHash = nDate;
						var providedEmail = regData.NewUserEmail.toString();
						var hashString = providedEmail + todaysDateForHash;
						var saltt = slt;
						var hash = md5(hashString + saltt);
						var newRedirectURL = "";
						newRedirectURL = newRedirectURL + "aptNum=" + regData.HouseNumber;
						newRedirectURL = newRedirectURL + "&firstName=" + nUserFirstName;
						newRedirectURL = newRedirectURL + "&lastName=" + nUserLastName;
						newRedirectURL = newRedirectURL + "&street=" + regData.UserStreet;
						newRedirectURL = newRedirectURL + "&email=" + regData.UserEmail;
						newRedirectURL = newRedirectURL + "&homePhone=" + nUserHomePhone;
						newRedirectURL = newRedirectURL + "&cellPhone=" + regData.NewUserPhone;
						newRedirectURL = newRedirectURL + "&dt=" + todaysDateForHash;
						newRedirectURL = newRedirectURL + "&city=" + regData.NewUserCity;
						newRedirectURL = newRedirectURL + "&state=" + regData.NewUserProvince;
						newRedirectURL = newRedirectURL + "&zip=" + regData.NewUserPostalCode;
						newRedirectURL = newRedirectURL + "&planName=" + regData.RatePeriod;
						newRedirectURL = newRedirectURL + "&hash=" + hash;
						window.location.assign(newRedirectURL);
					}

					else if (result['status'] == 'error') {
						alert("An error occured! Please check your filled out data.");
					}
					else if (result['status'] == 'error2') {
						alert("Validation error.");
					}
				},
			})
		};


