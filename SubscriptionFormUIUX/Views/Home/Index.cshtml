		function ConfirmEmailBlur() {

			// Email validation
			// 1.Check if the two emails are identical
			if ($("#txtBoxEmail").val() != $("#txtBoxConfirmEMail").val()) {
				alert("The EMAILS do not match ! Please re enter the email.");
				$("#txtBoxConfirmEMail").val('');
			}
			else {
				//2. If they are identical, check if the address is valid email format address
				var x = $("#txtBoxConfirmEMail").val();
				var atpos = x.indexOf("@@");
				var dotpos = x.lastIndexOf(".");
				if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) {
					alert("Not a valid e-mail address");
					$("#txtBoxConfirmEMail").val('');
					return false;
				}
			}
		}
