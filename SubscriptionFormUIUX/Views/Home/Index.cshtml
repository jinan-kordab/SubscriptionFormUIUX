		$.date = function (dateObject) {
			var d = new Date(dateObject);
			var day = d.getDate();
			var month = d.getMonth() + 1;
			var year = d.getFullYear();
			if (day < 10) {
				day = "0" + day;
			}
			if (month < 10) {
				month = "0" + month;
			}
			var date = year + "-" + month + "-" + day;

			return date;
		};

		function RegisterNew() {
			$("btnGetTime").prop('disabled', true);
			var slt = "";
			////////////////////get salt via ajax , then use it//////////////////////////////
			var xmlhttp = new XMLHttpRequest();
			xmlhttp.onreadystatechange = function () {
				if (this.readyState == 4 && this.status == 200) {
					var rData = this.responseText;
					slt = rData;

					alert("The salt is:"+ slt);
				}
			};
			xmlhttp.open("GET", "GetSalt.aspx?query=getmysalt&uval=" + new Date().getTime(), true);
			xmlhttp.send();
			/////////////////////////////////////////////////
			var nUserFirstName = $("#toSubmitFirstName").val();
			var nUserLastName = $("#toSubmitLastName").val();
			var nUserHomePhone = $("#txtBoxHomePhone").val();
			var usrName = $("#toSubmitFirstName").val() + " " + $("#toSubmitLastName").val();
			if (usrName.length > 50) {
				usrName = usrName.slice(0, 50);
			}
			var useracceptedRate = $("#lblPlanNameHeading").text();
			var useracceptedEmail = $("#toSubmitConfirmEmail").val();
			var useracceptedPhone = $("#toSubmitCellPhone").val();
			var useracceptedApt = $("#toSubmitApartmentNumber").val();
			var useracceptedStreet = $("#toSubmitStreetAddress").val();
			var useracceptedCity = $("#toSubmitCity").val();
			var useracceptedState = $("#toSubmitState").val();
			var useracceptedZip = $("#toSubmitZipCode").val();



			var regData = {
				CDateTime: new Date(),
				RatePeriod: $("#lblPlanNameHeading").text(),
				PhoneNumber: $("#toSubmitCellPhone").val(),
				HouseNumber: $("#toSubmitApartmentNumber").val(),
				ComplaintLateTime: $("#toSubmitCCSecurityCode").val(),
				CCNumber: $("#toSubmitCreditCard").val(),
				CCExpire: $("#toSubmitCCExpirationDate").val().toString(),
				UserName: usrName,
				UserDayPhone: $("#toSubmitCellPhone").val(),
				UserStreet: $("#toSubmitStreetAddress").val(),
				UserApartment: $("#toSubmitApartmentNumber").val(),
				UserCity: $("#toSubmitCity").val(),
				UserProvince: $("#toSubmitState").val(),
